{
  "name": "feathers-exx",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./server/start.js",
    "dev": "cross-env NODE_ENV=development DEBUG=app:* nodemon ./server/start.js -e js,json,twig --ignore client/",
    "css-mixins": "node-sass client/src/sass/mixins.scss -o client/public/css/",
    "css-themes": "node-sass client/src/sass/themes/ -o client/public/css/themes/",
    "css-build": "npm run css-mixins && npm run css-themes",
    "css-watch": "node-sass -w client/src/sass/ -o client/public/css/",
    "js-build": "webpack",
    "js-watch": "webpack --watch",
    "build": "npm run css-build && npm run js-build",
    "watch": "npm run css-watch | npm run js-watch",
    "start-mongod": "mongod -f config/db/mongod.config.yml",
    "start-rethinkdb": "rethinkdb --config-file ./config/db/rethinkdb.config",
    "heroku-postbuild": "npm run build"
  },
  "dependencies": {
    "@feathersjs/authentication": "^2.1.7",
    "@feathersjs/authentication-jwt": "^2.0.0",
    "@feathersjs/authentication-local": "^1.2.1",
    "@feathersjs/authentication-oauth1": "^1.0.3",
    "@feathersjs/authentication-oauth2": "^1.1.0",
    "@feathersjs/client": "^3.4.2",
    "@feathersjs/configuration": "^1.0.2",
    "@feathersjs/errors": "^3.3.0",
    "@feathersjs/express": "^1.2.0",
    "@feathersjs/feathers": "^3.1.0",
    "@feathersjs/socketio": "^3.2.0",
    "ajv": "^6.5.2",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "body-parser": "~1.18.2",
    "compression": "^1.7.3",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "cross-env": "^5.2.0",
    "debug": "^2.6.9",
    "dotenv": "^6.0.0",
    "dotenv-webpack": "^1.5.7",
    "elasticsearch": "^14.2.2",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "feathers-elasticsearch": "^1.3.1",
    "feathers-hooks-common": "^4.14.3",
    "feathers-knex": "^3.0.4",
    "feathers-localstorage": "^2.0.1",
    "feathers-memory": "^2.1.1",
    "feathers-mongodb": "^3.0.1",
    "feathers-mongoose": "^6.1.0",
    "feathers-nedb": "^3.0.0",
    "feathers-rethinkdb": "^0.5.1",
    "feathers-sequelize": "^3.1.0",
    "fetch-polyfill": "^0.8.2",
    "helmet": "^3.12.1",
    "highlight.js": "^9.12.0",
    "jstorage": "^0.4.8",
    "knex": "^0.15.2",
    "lodash": "^4.17.5",
    "moment": "^2.22.2",
    "mongodb": "^3.0.5",
    "mongoose": "^5.2.3",
    "morgan": "~1.9.0",
    "mysql": "^2.15.0",
    "mysql2": "^1.5.3",
    "nedb": "^1.8.0",
    "passport-facebook": "^2.1.1",
    "passport-github": "^1.1.0",
    "passport-google-oauth20": "^1.0.0",
    "passport-instagram": "^1.0.0",
    "passport-twitter": "^1.0.4",
    "rethinkdbdash": "^2.3.31",
    "sequelize": "^4.37.4",
    "serve-favicon": "~2.4.5",
    "socket.io-client": "^2.0.4",
    "sqlite3": "^4.0.2",
    "twig": "~0.10.3",
    "unhandled-rejection": "^1.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.3",
    "babel-preset-env": "^1.6.1",
    "bulma": "^0.6.2",
    "bulmaswatch": "^0.6.1",
    "css-loader": "^0.28.10",
    "node-sass": "^4.9.2",
    "nodemon": "^1.18.1",
    "style-loader": "^0.20.2",
    "twig-loader": "^0.3.1",
    "webpack": "3.11.0"
  }
}
