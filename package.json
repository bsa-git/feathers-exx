{
  "name": "feathers-exx",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./server/start.js",
    "dev": "nodemon ./server/start.js",
    "css-mixins": "node-sass client/src/sass/mixins.scss -o client/public/css/",
    "css-themes": "node-sass client/src/sass/themes/ -o client/public/css/themes/",
    "css-build": "npm run css-mixins && npm run css-themes",
    "css-watch": "node-sass -w client/src/sass/ -o client/public/css/",
    "js-build": "webpack",
    "js-watch": "webpack --watch",
    "build-client": "npm run css-build && npm run js-build",
    "watch-client": "npm run css-watch | npm run js-watch",
    "start-mongod": "mongod -f config/mongod.config.yml",
    "start-elasticsearch": "elasticsearch"

  },
  "dependencies": {
    "@feathersjs/client": "^3.4.2",
    "@feathersjs/errors": "^3.3.0",
    "@feathersjs/express": "^1.2.0",
    "@feathersjs/feathers": "^3.1.0",
    "@feathersjs/socketio": "^3.2.0",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "body-parser": "~1.18.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "elasticsearch": "^14.2.2",
    "express": "~4.15.5",
    "feathers-elasticsearch": "^1.1.0",
    "feathers-knex": "^3.0.4",
    "feathers-localstorage": "^2.0.1",
    "feathers-memory": "^2.1.1",
    "feathers-mongodb": "^3.0.1",
    "feathers-mongoose": "^6.1.0",
    "feathers-nedb": "^3.0.0",
    "feathers-sequelize": "^3.1.0",
    "fetch-polyfill": "^0.8.2",
    "highlight.js": "^9.12.0",
    "jstorage": "^0.4.8",
    "knex": "^0.14.4",
    "location-helper": "^1.0.3",
    "lodash": "^4.17.5",
    "moment": "^2.20.1",
    "mongodb": "^3.0.5",
    "mongoose": "^5.0.12",
    "morgan": "~1.9.0",
    "mysql": "^2.15.0",
    "mysql2": "^1.5.3",
    "nedb": "^1.8.0",
    "npm": "^5.7.1",
    "sequelize": "^4.37.4",
    "serve-favicon": "~2.4.5",
    "socket.io-client": "^2.0.4",
    "sqlite3": "^4.0.0",
    "twig": "~0.10.3",
    "unhandled-rejection": "^1.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.3",
    "babel-preset-env": "^1.6.1",
    "bulma": "^0.6.2",
    "bulmaswatch": "^0.6.1",
    "css-loader": "^0.28.10",
    "node-sass": "^4.7.2",
    "nodemon": "^1.14.11",
    "style-loader": "^0.20.2",
    "twig-loader": "^0.3.1",
    "webpack": "3.11.0"
  }
}
