{
  "name": "feathers-exx",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start-server": "node ./server/start.js",
    "dev": "nodemon ./server/start.js",
    "css-build": "node-sass client/src/sass/themes/ -o client/public/css/themes/",
    "css-deploy": "npm run css-build",
    "css-watch": "node-sass -w client/src/sass/ -o client/public/css/",
    "deploy-client": "npm run css-deploy && npm run js-build",
    "js-build": "webpack",
    "js-watch": "webpack --watch",
    "start-client": "npm run css-watch | npm run js-watch"
  },
  "dependencies": {
    "@feathersjs/client": "^3.4.2",
    "@feathersjs/errors": "^3.3.0",
    "@feathersjs/express": "^1.2.0",
    "@feathersjs/feathers": "^3.1.0",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "body-parser": "~1.18.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.15.5",
    "fetch-polyfill": "^0.8.2",
    "highlight.js": "^9.12.0",
    "jstorage": "^0.4.8",
    "location-helper": "^1.0.3",
    "moment": "^2.20.1",
    "morgan": "~1.9.0",
    "serve-favicon": "~2.4.5",
    "socket.io-client": "^2.0.4",
    "twig": "~0.10.3",
    "unhandled-rejection": "^1.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.3",
    "babel-preset-env": "^1.6.1",
    "bulma": "^0.6.2",
    "bulmaswatch": "^0.6.1",
    "css-loader": "^0.28.10",
    "node-sass": "^4.7.2",
    "nodemon": "^1.14.11",
    "style-loader": "^0.20.2",
    "twig-loader": "^0.3.1",
    "webpack": "^4.0.1",
    "webpack-cli": "^2.0.9"
  }
}
